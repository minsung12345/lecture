package webkms.emp;

public class PartTimeWorker extends Employee{
	private static int salary;
	private int workingHours;
	private int salaryTime;
	public PartTimeWorker(int businessNum, String name, String department, int salaryTime,int workingHours) {
		super(businessNum, name, department,salary);
		this.salaryTime = salaryTime;
		this.workingHours =workingHours;
	}
	@Override
	public String toString() {
		return getName()+"의 급여는: "+salaryTime*workingHours+"원";	
	}
	
}
