package webkms;

public class Circle extends Shape{
	private double height;
	private double base;
	
	public Circle(String name, String color,
			int X, int Y, double height, double base) {
		super(name,color,X,Y);
		this.height= height;
		this.base=base;
		
	}
	public String display() {
		return super.display() + ", 높이:"+height+", 밑변:"+base;
	}
	
}
