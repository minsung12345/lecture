package webkms.emp;

public class EmployeeMain {

	public static void main(String[] args) {
		FullTimeEmployee full = new FullTimeEmployee(1801126,"김민성","IT",800000);
		System.out.println(full.toString());
		PartTimeWorker part = new PartTimeWorker(1801126,"김민성","IT",9980,8);
		System.out.println(part.toString()); 
		
	}

}
