package webkms.emp;

public class PartTimeWorker extends Employee{
	private int workingHours;
	public PartTimeWorker(int businessNum, String name, String department, int salary,int workingHours) {
		super(businessNum, name, department, salary);
		this.workingHours =workingHours;
	}
	@Override
	public String toString() {
		return getName()+"의 급여는: "+getSalary()*workingHours+"원";	
	}
	
}
