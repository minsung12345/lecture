package Card;

import webkms.Dealer;

public class CardMain {

	public static void main(String[] args) {
		Dealer 주윤발 = new Dealer(); //딜러 생성
		System.out.println("섞기 전 카드: ");
		System.out.println(주윤발.displayCard());
		
		주윤발.shuffle();
		System.out.println("섞은 후 카드: ");
		System.out.println(주윤발.displayCard());
	}

}
