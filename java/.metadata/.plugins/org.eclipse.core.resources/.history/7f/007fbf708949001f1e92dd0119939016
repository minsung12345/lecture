package webkms.emp;

public ab class Employee {
 private String empNo;
 private String name;
 private int deptNo;
 private int salary;

 public Employee(String empNo, String name, int deptNo, int salary){
	 this.empNo = empNo;
	 this.name = name;
	 this.deptNo=deptNo;
	 this.salary = salary;
 }

public String getEmpNo() {
	return empNo;
}

public String getName() {
	return name;
}

public int getdeptNo() {
	return deptNo;
}

public int getSalary() {
	return salary;
}

@Override
public String toString() {
	return "Employee [사번=" + empNo + ", name=" + name + ", deptNo=" + deptNo + ", salary="
			+ salary + "]";
}



}

